#BANDAI-FCG-2, uses $600x for registers
#名門第三野球部, ドラゴンボール3 - 悟空伝
MAPPER 16
CPU_ROMSIZE 0x40000
PPU_ROMSIZE 0x40000

DUMP_START
STEP_START i 0 0xf 1
	CPU_WRITE $6008 i
	CPU_READ $8000 0x4000
STEP_END
CPU_READ $c000 0x4000

PPU_RAMTEST
STEP_START i 0 0x100 8
	CPU_WRITE $6000 i
	CPU_WRITE $6001 i | 1
	CPU_WRITE $6002 i | 2
	CPU_WRITE $6003 i | 3
	CPU_WRITE $6004 i | 4
	CPU_WRITE $6005 i | 5
	CPU_WRITE $6006 i | 6
	CPU_WRITE $6007 i | 7
	PPU_READ 0 0x2000
STEP_END
DUMP_END
