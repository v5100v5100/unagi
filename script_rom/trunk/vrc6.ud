#VRCVI A0,A1 normal,reverse ¶¦ÄÌÈÇ
#use address (A0 == 0 and A1 == 0) or (A1 == 1 and A1 == 1)
#RC845, RC846, RC861
MAPPER 24
CPU_ROMSIZE 0x40000
PPU_ROMSIZE 0x40000
DUMP_START
STEP_START i 0 0x0f 1
	CPU_WRITE $8000 i
	CPU_READ $8000 0x4000
STEP_END
CPU_WRITE $c000 $1e
CPU_READ $c000 0x2000
CPU_READ $e000 0x2000

CPU_WRITE $b003 $20
STEP_START v 0 0x100 4
	CPU_WRITE $d000 v | 0
	CPU_WRITE $d003 v | 1
	CPU_WRITE $e000 v | 2
	CPU_WRITE $e003 v | 3
	PPU_READ 0 0x400
	PPU_READ 0x0c00 0x800
	PPU_READ 0x1c00 0x400
STEP_END
DUMP_END
