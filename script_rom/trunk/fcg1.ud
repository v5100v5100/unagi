#BANDAI-FCG-1, uses $7FFx for registers
#名門第三野球部, ドラゴンボール3 - 悟空伝
MAPPER 16
CPU_ROMSIZE 0x40000
PPU_ROMSIZE 0x40000

DUMP_START
STEP_START i 0 0xf 1
	CPU_WRITE $7ff8 i
	CPU_READ $8000 0x4000
STEP_END
CPU_READ $c000 0x4000

PPU_RAMTEST
STEP_START i 0 0x100 8
	CPU_WRITE $7ff0 i
	CPU_WRITE $7ff1 i | 1
	CPU_WRITE $7ff2 i | 2
	CPU_WRITE $7ff3 i | 3
	CPU_WRITE $7ff4 i | 4
	CPU_WRITE $7ff5 i | 5
	CPU_WRITE $7ff6 i | 6
	CPU_WRITE $7ff7 i | 7
	PPU_READ 0 0x2000
STEP_END
DUMP_END
