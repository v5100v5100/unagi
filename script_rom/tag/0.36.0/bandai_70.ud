#07 ポケットザウルス 十王剣の謎
#聖闘士星矢 黄金伝説
MAPPER 70
CPU_ROMSIZE 0x20000
PPU_ROMSIZE 0x20000

DUMP_START
STEP_START i 0 8 1
	CPU_WRITE $c000 i << 4
	CPU_READ $8000 0x4000
STEP_END
#Bus-Conflicts, memory at C000h-C0FFh should be filled by 00h-FFh.
#CPU_READ $c000 0x4000

STEP_START i 0 0x10 1
	CPU_WRITE $c000 i
	PPU_READ 0 0x2000
STEP_END
DUMP_END
